+                            onClick={() => window.open(exp.file_url, '_blank')}
+                            className="bg-purple-600 hover:bg-purple-700 gap-2"
+                          >
+                            <Download className="w-4 h-4" />
+                            Download
+                          </Button>
+                        )}
+                      </div>
+                    </div>
+                  </CardContent>
+                </Card>
+              ))}
+            </div>
+          )}
+        </div>
+      </OnboardingGuard>
+    </AuthGuard>
+  );
+}
