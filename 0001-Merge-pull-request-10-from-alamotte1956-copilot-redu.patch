+
+  // Optimized change handlers with sanitization
+  const handleInputChange = useCallback((field, sanitizeOptions = {}) => (e) => {
+    const value = sanitize(e.target.value, { type: 'text', ...sanitizeOptions });
+    setFormData(prev => ({ ...prev, [field]: value }));
